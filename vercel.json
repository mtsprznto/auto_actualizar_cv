{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "api/routers/**",
                    "api/models/**",
                    "api/utils/**",
                    "api/__init__.py"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}